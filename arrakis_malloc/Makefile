NAME	=	libam.a

INCLU	=	../../include

OBJ	=	$(SRC:.c=.o)

all:	$(NAME)

all:	$(NAME)

$(NAME):	$(OBJ)
	ar rc $(NAME) $(OBJ)

clean:
	rm -f src/*.o

fclean: clean
	rm -f $(NAME)

re:	fclean all
